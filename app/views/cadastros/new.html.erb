<% provide(:current_page, 'Cadastro de Adesão de Número do WhatsApp') %>

<div class="row">
  <div class="col-xs-12">
    <!-- general form elements -->
    <div class="box box-primary">
      <div class="box-header with-border">
        <h3 class="box-title">Cadastro de Adesão de Número do WhatsApp</h3>
      </div>

      <%= form_for @cadastro, html: {role: 'form'} do |f| %>
          <div class="box-body">
            <%= render 'form', f: f, cadastro: @cadastro%>


            <div class="box-footer">
              <button type="submit" class="btn btn-primary">Salvar</button>
            </div>
          </div>
      <% end %>
    </div>
    <!-- /.box -->

  </div>
</div>
