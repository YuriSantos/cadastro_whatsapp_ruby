<p id="notice"><%= notice %></p>

<h1>Partes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partes.each do |parte| %>
      <tr>
        <td><%= parte.nome %></td>
        <td><%= link_to 'Show', parte %></td>
        <td><%= link_to 'Edit', edit_parte_path(parte) %></td>
        <td><%= link_to 'Destroy', parte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parte', new_parte_path %>
