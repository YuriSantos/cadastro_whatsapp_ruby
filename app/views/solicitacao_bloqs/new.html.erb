<% provide(:current_page, 'Solicitação de Bloqueio de usuario') %>

<div class="row">
  <div class="col-xs-12">
    <!-- general form elements -->
    <div class="box box-primary">
      <div class="box-header with-border">
        <h3 class="box-title">Solicitação de Bloqueio de usuário</h3>
      </div>

      <%= form_for @solicitacao_bloq, html: {role: 'form'} do |f| %>
          <div class="box-body">
            <%= render 'form', f: f, solicitacao_bloq: @solicitacao_bloq%>


            <div class="box-footer">
              <button type="submit" class="btn btn-primary">Solicitar</button>
            </div>
          </div>
      <% end %>
    </div>
    <!-- /.box -->

  </div>
</div>